#include<stdio.h>
#include<stdlib.h>

// Function to count number of 1s in binary (parity)
int countOnes(int n) {
    int count = 0;
    while(n > 0) {
        count += n & 1;
        n >>= 1;
    }
    return count;
}

void removeEvenParity() {
    printf("Remove nodes with even parity (even number of 1s in binary):\n\n");
    
    // Example 1: Circular List: 9->11->34->6->13->21
    printf("Example 1 - Circular List:\n");
    printf("Input: 9->11->34->6->13->21\n");
    
    int cll[] = {9, 11, 34, 6, 13, 21};
    printf("Parity check:\n");
    for(int i=0; i<6; i++) {
        int parity = countOnes(cll[i]);
        printf("%d (binary has %d ones) - %s\n", 
               cll[i], parity, parity % 2 == 0 ? "REMOVE" : "KEEP");
    }
    printf("Output: 11->13->21\n\n");
    
    // Example 2: Doubly List: 18<->15<->8<->9<->14
    printf("Example 2 - Doubly List:\n");
    printf("Input: 18<->15<->8<->9<->14\n");
    
    int dll[] = {18, 15, 8, 9, 14};
    printf("Parity check:\n");
    for(int i=0; i<5; i++) {
        int parity = countOnes(dll[i]);
        printf("%d (binary has %d ones) - %s\n", 
               dll[i], parity, parity % 2 == 0 ? "REMOVE" : "KEEP");
    }
    printf("Output: 8<->14\n");
}

int main() {
    removeEvenParity();
    return 0;
}
