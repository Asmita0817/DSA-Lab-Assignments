#include<stdio.h>

int main() {
    int arr[5] = {1, 2, 3, 4, 5};
    int i, temp;
    
    printf("Original array: ");
    for(i=0; i<5; i++) {
        printf("%d ", arr[i]);
    }
    
    // Reverse the array
    for(i=0; i<2; i++) {
        temp = arr[i];
        arr[i] = arr[4-i];
        arr[4-i] = temp;
    }
    
    printf("\nReversed array: ");
    for(i=0; i<5; i++) {
        printf("%d ", arr[i]);
    }
    
    return 0;
}