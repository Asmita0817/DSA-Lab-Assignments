#include<stdio.h>

int main() {
    int A[2][2] = {{1,2}, {3,4}};
    int B[2][2] = {{5,6}, {7,8}};
    int C[2][2] = {0};
    int i, j, k;
    
    printf("Matrix A:\n");
    for(i=0; i<2; i++) {
        for(j=0; j<2; j++) {
            printf("%d ", A[i][j]);
        }
        printf("\n");
    }
    
    printf("\nMatrix B:\n");
    for(i=0; i<2; i++) {
        for(j=0; j<2; j++) {
            printf("%d ", B[i][j]);
        }
        printf("\n");
    }
    
    // Multiply matrices
    for(i=0; i<2; i++) {
        for(j=0; j<2; j++) {
            for(k=0; k<2; k++) {
                C[i][j] += A[i][k] * B[k][j];
            }
        }
    }
    
    printf("\nResult A x B:\n");
    for(i=0; i<2; i++) {
        for(j=0; j<2; j++) {
            printf("%d ", C[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}