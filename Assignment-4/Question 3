#include<stdio.h>
#define SIZE 6

int main() {
    int queue[] = {4, 7, 11, 20, 5, 9};
    int n = 6;
    int temp[6];
    
    printf("Original queue: ");
    for(int i=0; i<n; i++) {
        printf("%d ", queue[i]);
    }
    
    // Interleave first half with second half
    for(int i=0; i<n/2; i++) {
        temp[2*i] = queue[i];
        temp[2*i + 1] = queue[i + n/2];
    }
    
    printf("\nInterleaved queue: ");
    for(int i=0; i<n; i++) {
        printf("%d ", temp[i]);
    }
    printf("\n");
    
    return 0;
}
