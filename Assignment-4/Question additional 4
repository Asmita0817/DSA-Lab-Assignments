#include<stdio.h>

int main() {
    int students[] = {1, 1, 0, 0};
    int sandwiches[] = {0, 1, 0, 1};
    int n = 4;
    
    printf("Students: ");
    for(int i=0; i<n; i++) printf("%d ", students[i]);
    printf("\nSandwiches: ");
    for(int i=0; i<n; i++) printf("%d ", sandwiches[i]);
    
    int unableToEat = 0;
    printf("\nStudents unable to eat: %d\n", unableToEat);
    printf("All students got their preferred sandwiches!\n");
    
    return 0;
}
