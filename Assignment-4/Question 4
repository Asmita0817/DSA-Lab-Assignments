#include<stdio.h>
#include<string.h>

int main() {
    char str[] = "aabc";
    int count[26] = {0};
    
    printf("Input: %s\n", str);
    printf("Output: ");
    
    for(int i=0; i<strlen(str); i++) {
        count[str[i]-'a']++;
        
        // Find first non-repeating character
        int found = 0;
        for(int j=0; j<=i; j++) {
            if(count[str[j]-'a'] == 1) {
                printf("%c ", str[j]);
                found = 1;
                break;
            }
        }
        if(!found) printf("-1 ");
    }
    printf("\n");
    
    return 0;
}
