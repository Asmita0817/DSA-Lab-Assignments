#include<stdio.h>

int main() {
    int arr[] = {1, 2, 3, 4, 6, 7, 8}; // 5 is missing
    int n = 7;
    
    printf("Array: ");
    for(int i=0; i<n; i++) {
        printf("%d ", arr[i]);
    }
    
    // Linear search for missing number
    for(int i=0; i<n; i++) {
        if(arr[i] != i+1) {
            printf("\nMissing number: %d", i+1);
            break;
        }
    }
    
    return 0;
}
