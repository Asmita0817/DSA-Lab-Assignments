#include<stdio.h>

int main() {
    int arr[] = {1, 2, 3, 4, 6, 7, 8}; // 5 is missing
    int n = 7;
    int low = 0, high = n-1, mid;
    
    printf("Array: ");
    for(int i=0; i<n; i++) {
        printf("%d ", arr[i]);
    }
    
    // Binary search for missing number
    while(low <= high) {
        mid = (low + high) / 2;
        
        if(arr[mid] == mid+1) {
            low = mid + 1;
        }
        else {
            high = mid - 1;
        }
    }
    
    printf("\nMissing number: %d", low+1);
    return 0;
}
